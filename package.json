{
  "name": "manhattan-project",
  "version": "1.0.0",
  "description": "A MERN stack application for connection people with their poison",
  "main": "server.ts",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dest/server.js",
    "build-ts": "tsc",
    "postinstall": "npm run build-ts",
    "server": "nodemon -x ts-node server.ts",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "test": "jest --config ./jest.config.js",
    "packages": "npm install && cd client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmcxii/manhattan-project.git"
  },
  "author": "Team Taco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmcxii/manhattan-project/issues"
  },
  "homepage": "https://github.com/mmcxii/manhattan-project#readme",
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/jest": "^24.0.19",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongodb-memory-server": "^1.8.0",
    "@types/mongoose": "^5.5.19",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.11",
    "@types/sharp": "^0.23.0",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "aws-sdk": "^2.556.0",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "sharp": "^0.23.1",
    "socket.io": "^2.3.0",
    "ts-jest": "^24.1.0",
    "ts-mongoose": "0.0.19",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "concurrently": "^4.1.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "mongodb-memory-server-global": "^6.0.1",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1"
  }
}
